<div class="card">
    <div class="card-header">
        Feedback Details
    </div>

    <div class="card-body">
        <div class="mb-2">
            <table class="table table-bordered table-striped">
                <tbody>
                    <tr>
                        <th>
                            Id
                        </th>
                        <td>
                            <%= feedback.id %>
                        </td>
                    </tr>
                    <tr>
                        <th>
                            Name
                        </th>
                        <td>
                            <%= feedback.name %>
                        </td>
                    </tr>
                    <tr>
                        <th>
                            Email
                        </th>
                        <td>
                            <%= feedback.email %>
                        </td>
                    </tr>
                    <tr>
                        <th>
                            Email
                        </th>
                        <td>
                            <%- feedback.comment %>
                        </td>
                    </tr>
                    <tr>
                        <th>
                            File
                        </th>
                        <td>

                            <a class="badge badge-info" href="/admin/files/<%= feedback.file %>" target="_blank">
                                <%= feedback.file %>
                            </a>
                        </td>
                    </tr>
                </tbody>
            </table>
            <a style="margin-top:20px;" class="btn btn-default" href="/user/feedbacks">
                Back
            </a>
        </div>

        <nav class="mb-3">
            <div class="nav nav-tabs">
            </div>
        </nav>
        <div class="tab-content">

        </div>
    </div>
</div>

<%- contentFor('style') %>