<div class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                    Dashboard
                </div>
                <div class="card-body">
                    <% if(status) { %>
                        <div class="alert alert-success" role="alert">
                            <%= status %>
                        </div>
                        <% } %>
                            <div class="row">
                                <div class="col-lg-3 col-xs-6">
                                    <div class="small-box bg-aqua">
                                        <div class="inner">
                                            <h3>
                                                <%= users %>
                                            </h3>

                                            <p>New Users</p>
                                        </div>
                                        <div class="icon">
                                            <i class="ion ion-bag"></i>
                                        </div>
                                        <a href="/admin/users" class="small-box-footer">More info
                                            <i class="fa fa-arrow-circle-right"></i></a>
                                    </div>
                                </div>
                            </div>
                </div>
            </div>
        </div>
    </div>

    <%- contentFor('style') %>